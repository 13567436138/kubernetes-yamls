apiVersion: v1
kind: Service
metadata:
  labels:
    app: ks-console
    tier: frontend
    version: advanced-2.0.0
  name: ks-console
  namespace: kubesphere-system
spec:
  ports:
  - name: nginx
    port: 80
    protocol: TCP
    targetPort: 8000
    {% if console_port != None -%}
    nodePort: {{ console_port }}
    {%- endif %}
    
  selector:
    app: ks-console
    tier: frontend
    version: advanced-2.0.0
  type: NodePort